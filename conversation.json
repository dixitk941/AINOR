{
  "commands": [
    {
      "category": "greetings",
      "patterns": ["hello", "hi", "hey", "namaste", "hola"],
      "responses": ["Hello there!", "Hi!", "Greetings!", "Howdy!", "Namaste!"]
    },
    {
      "category": "how_are_you",
      "patterns": ["how are you", "how's it going", "what's up"],
      "responses": ["I'm doing great, thanks for asking!", "All systems are operational!", "I am here and ready!", "Feeling awesome, how about you?"]
    },
    {
      "category": "farewell",
      "patterns": ["goodbye", "bye", "go offline"],
      "responses": ["Goodbye!", "See you later!", "Going offline.", "Take care!"]
    },
    {
      "category": "time",
      "patterns": ["what time is it", "tell me the time", "current time"],
      "responses": ["It's {time}.", "The time is currently {time}.", "Right now, it's {time}."]
    },
    {
      "category": "date",
      "patterns": ["what's the date", "tell me the date", "current date"],
      "responses": ["Today's date is {date}.", "The current date is {date}.", "It's {date} today."]
    },
    {
      "category": "search_google",
      "patterns": ["search google for", "google for", "find on google"],
      "responses": ["Here are the search results for {query}:", "I found this for you on Google:", "Check out these results for {query}:"]
    },
    {
      "category": "open_website",
      "patterns": ["open", "launch", "go to"],
      "responses": ["Opening {site}.", "Launching {site} for you.", "Taking you to {site}."]
    },
    {
      "category": "jokes",
      "patterns": ["tell me a joke", "make me laugh", "joke"],
      "responses": [
        "Why don't scientists trust atoms? Because they make up everything!",
        "Why did the math book look sad? Because it had too many problems.",
        "Why don’t skeletons fight each other? They don’t have the guts!"
      ]
    },
    {
      "category": "unknown_command",
      "patterns": ["*"],
      "responses": ["I'm sorry, I didn't understand that.", "Could you repeat that?", "I didn't get that. Could you clarify?"]
    }
  ]
}
